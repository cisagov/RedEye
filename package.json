{
	"name": "redeye",
	"productName": "RedEye",
	"version": "0.8.1",
	"private": true,
	"workspaces": [
		"applications/**",
		"packages/**"
	],
	"bin": {
		"redeye-cs-parser": "./dist/packages/cs-parser/src/index.js"
	},
	"scripts": {
		"postinstall": "yarn run husky:init",
		"start": "yarn run start:dev",
		"test": "yarn cy:open",
		"build:all": "yarn nx run-many --target=build --all",
		"release": "yarn nx run-many --target=release --all",
		"release:all": "yarn nx run-many --target=release-all --all",
		"format": "prettier --ignore-path .gitignore \"**/*.+(ts|tsx|json)\" --write",
		"lint": "yarn nx run-many --target=lint --all --fix",
		"husky:init": "husky install && husky set .husky/pre-commit \"yarn lint-staged --relative\"",
		"create-migrations": "yarn nx create-migrations migrations && yarn nx migrate-dbs migrations",
		"add-changeset": "yarn changeset",
		"start:client": "yarn nx serve client",
		"start:server": "yarn nx serve server",
		"start:dev": "yarn nx run-many --target=serve --projects=graph,server,client",
		"start:graph": "yarn nx serve graph",
		"combine:reports": "jrm dist/applications/redeye-e2e/results/combined-report.xml \"dist/applications/redeye-e2e/results/*.xml\"",
		"cy:open": "yarn nx open-cy redeye-e2e",
		"cy:open-blue": "yarn nx run redeye-e2e:open-cy-blue",
		"start:blue": "SERVER_BLUE_TEAM=true yarn run start:dev"
	},
