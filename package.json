{
	"name": "redeye",
	"productName": "RedEye",
	"version": "0.8.5",
	"private": true,
	"workspaces": [
		"applications/**",
		"packages/**"
	],
	"bin": {
		"redeye-cs-parser": "./packages/cs-parser/dist/index.js"
	},
	"scripts": {
		"moon": "$(yarn bin moon)",
		"postinstall": "yarn run husky:init",
		"start": "yarn run start:dev",
		"build": "yarn moon run :build",
		"test": "yarn cy:open",
		"format": "yarn moon run :format",
		"lint": "yarn moon run @redeye/client:lint",
		"husky:init": "husky install && husky set .husky/pre-commit \"yarn lint-staged --relative\"",
		"create-migrations": "yarn moon run @redeye/migrations:create-migrations && yarn moon run @redeye/migrations:migrate-dbs",
		"add-changeset": "yarn changeset",
		"start:client": "yarn moon run @redeye/client:start-dev",
		"start:server": "yarn moon run @redeye/server:start-dev",
		"start:dev": "yarn moon run @redeye/server:start-dev @redeye/client:start-dev",
		"start:graph": "yarn moon run @redeye/graph:start-vite",
		"combine:reports": "jrm dist/applications/redeye-e2e/results/combined-report.xml \"dist/applications/redeye-e2e/results/*.xml\"",
		"cy:open": "yarn moon run @redeye/e2e:open-cy",
		"cy:open-blue": "yarn moon run @redeye/e2e:open-cy-blue",
		"start:blue": "SERVER_BLUE_TEAM=true yarn run start:dev",
		"release:all": "yarn moon run server:release-all && yarn moon run cs-parser:release-all",
		"release:mac": "yarn moon run server:release-mac cs-parser:release-mac",
		"release:linux": "yarn moon run server:release-linux cs-parser:release-linux",
		"release:windows": "yarn moon run server:release-windows cs-parser:release-windows"
	},
